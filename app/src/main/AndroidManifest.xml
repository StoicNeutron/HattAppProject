<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samnangthorn.hatt_app10">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="Hatt App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hatt_App10">
        <activity android:name=".Timer"></activity>
        <activity android:name=".Schedule" />
        <activity android:name=".Exercise" />
        <activity android:name=".Report" />
        <activity android:name=".LogIn" />
        <activity android:name=".SignUp" />
        <activity android:name=".Setup" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>